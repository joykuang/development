{
    "name": "joykuang/tp51",
    "description": "Project tp51 created in learning php",
    "type": "project",
    "keywords": [
        "tp51",
        "joykuang",
        "thinkphp51"
    ],
    "homepage": "http://joykuang.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Joy Kuang",
            "email": "q-cute@163.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "topthink/framework": "~5.1.0@dev",
        "aliyuncs/oss-sdk-php": "^2.2"
    },
    "require-dev": {
        "filp/whoops": "^2.1"
    },
    "autoload": {
        "classmap": ["local/module"],
        "psr-4": {
            "app\\": "local"
        }
    },
    "config": {
        "preferred-install": "dist",
        "vendor-dir" : "packages"
    },
    "extra": {
        "think-path": "packages/topthink/framework"
    },
    "archive": {
        "exclude": [
            "packages",
            "runtime",
            "public/assets/3rd-party"
        ]
    }
}
