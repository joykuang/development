{
    "name": "joykuang/tp51",
    "description": "Project tp51 created in learning php",
    "type": "project",
    "keywords": [
        "tp51",
        "joykuang",
        "thinkphp51"
    ],
    "homepage": "http://joykuang.com",
    "license": "MIT",
    "authors": [{
        "name": "Joy Kuang",
        "email": "q-cute@163.com"
    }],
    "require": {
        "php": ">=5.6.0",
        "topthink/framework": "~5.1.0@dev",
        "aliyuncs/oss-sdk-php": "^2.2",
        "illuminate/container": "^5.5",
        "doctrine/orm": "^2.5"
    },
    "require-dev": {
        "filp/whoops": "^2.1",
        "fzaninotto/faker": "^1.7"
    },
    "require-backup": {
        "guzzlehttp/guzzle": "^6.3",
        "symfony/orm-pack": "^1.0"
    },
    "autoload": {
        "classmap": [
            "thoughts/module",
            "thoughts/system"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "vendor-dir": "packages"
    },
    "extra0": {
        "think-path": "packages/topthink/framework"
    },
    "archive": {
        "exclude": [
            "packages",
            "runtime",
            "public/assets/3rd-party"
        ]
    },
    "scripts": {
        "command__BAK": [
            "YRS\\Console\\ComposerScripts::command"
        ],
        "pre-install-cmd": [
            "YRS\\Console\\ComposerScripts::preUpdate"
        ],
        "pre-update-cmd": [
            "YRS\\Console\\ComposerScripts::preUpdate"
        ],
        "post-update-cmd": [],
        "post-autoload-dump__BAK": [
            "YRS\\Console\\ComposerScripts::postAutoloadDump"
        ]
    }
}
