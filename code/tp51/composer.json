{
    "name": "joykuang/tp51",
    "description": "Project tp51 created in learning php",
    "type": "project",
    "keywords": [
        "tp51",
        "joykuang",
        "thinkphp51"
    ],
    "homepage": "http://joykuang.com",
    "license": "MIT",
    "authors": [{
        "name": "Joy Kuang",
        "email": "q-cute@163.com"
    }],
    "require": {
        "php": ">=5.6.0",
        "topthink/framework": "~5.1.0@dev",
        "aliyuncs/oss-sdk-php": "^2.2",
        "illuminate/container": "^5.5",
        "doctrine/orm": "^2.5"
    },
    "require-dev": {
        "filp/whoops": "^2.1"
    },
    "require-backup": {
        "guzzlehttp/guzzle": "^6.3",
        "symfony/orm-pack": "^1.0"
    },
    "autoload": {
        "classmap": [
            "local/core",
            "local/module"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "vendor-dir": "packages"
    },
    "extra": {
        "think-path": "packages/topthink/framework"
    },
    "archive": {
        "exclude": [
            "packages",
            "runtime",
            "public/assets/3rd-party"
        ]
    },
    "scripts-backup": {
        "post-update-cmd": [],
        "post-autoload-dump": [
            "YRS\\Console\\ComposerScripts::postAutoloadDump"
        ]
    }
}
